# Public URLs (safe to expose)
NEXT_PUBLIC_REACT_APP_AUTH_URL=https://auth.example.com
NEXT_PUBLIC_API_URL_KROOLO_AI=https://ai-api.example.com
NEXT_PUBLIC_API_URL=https://api.example.com
NEXT_PUBLIC_REACT_APP_API_URL=https://api.example.com
NEXT_PUBLIC_REACT_APP_API_URL_KROOLO_AI=https://ai-api.example.com
NEXT_PUBLIC_DJANGO_API_BASE_URL=https://django-api.example.com
NEXT_PUBLIC_BASE_URL_WS=wss://django-api.example.com
NEXT_PUBLIC_APP_URL=http://localhost:3000/
NEXT_PUBLIC_GEMINI_API_KEY=your_public_gemini_key

# OpenAI (client-side optional)
NEXT_PUBLIC_OPENAI_API_KEY=your_public_openai_key

# Backend and internal services
BACKEND_URL=https://backend.example.com

# MongoDB
MONGODB_URI=mongodb://user:pass@host:27017/poligap?authSource=admin
DB_NAME=poligap
MONGODB_ENTERPRISE_SEARCH_URI=mongodb://user:pass@host:27017/enterprise?authSource=admin

# AWS S3
AWS_REGION=us-east-2
AWS_ACCESS_KEY_ID=YOUR_ACCESS_KEY_ID
AWS_SECRET_ACCESS_KEY=YOUR_SECRET_ACCESS_KEY
AWS_BUCKET_NAME=your-bucket-name

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Portkey AI
PORTKEY_API_KEY=your-portkey-api-key

# Google Gemini (server-side)
GEMINI_API_KEY=your-gemini-api-key

# OpenAI (server-side)
OPENAI_API_KEY=your-openai-api-key
# Optional overrides
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL=https://api.openai.com/v1

# Pipedream
PIPEDREAM_ENVIRONMENT=production
PIPEDREAM_PROJECT_ID=your-pd-project-id
PIPEDREAM_CLIENT_ID=your-pd-client-id
PIPEDREAM_CLIENT_SECRET=your-pd-client-secret

# Node environment
NODE_ENV=development
